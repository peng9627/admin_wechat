<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-access="field">
    <class name="game.domain.model.user.User" table="account" lazy="true">
        <id name="userId" type="java.lang.Integer" column="id" unsaved-value="null">
        </id>

        <property name="account" column="account_name" type="java.lang.String"/>
    </class>
</hibernate-mapping>
