<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-access="field">
    <class name="game.domain.model.user.UserParent" table="t_user_rebate" lazy="true">
        <id name="id" type="java.lang.String" column="id" length="32">
            <generator class="uuid"/>
        </id>

        <version column="version" name="version" type="java.lang.Integer"/>

        <property name="createDate" column="create_date" type="java.util.Date"/>

        <property name="userId" column="user_id" type="java.lang.Integer" not-null="true" unique="true"/>

        <property name="parent" column="parent_id" type="java.lang.Integer"/>

        <property name="b" column="b" type="java.lang.Integer"/>

        <property name="a" column="a" type="java.lang.Integer"/>

        <property name="level" column="level" type="java.lang.Integer"/>

        <property name="area" column="area" type="java.lang.Integer"/>

        <property name="commission" type="java.math.BigDecimal" scale="2">
            <column name="commission" default="0"/>
        </property>

        <property name="totalCommission" type="java.math.BigDecimal" scale="2">
            <column name="total_commission" default="0"/>
        </property>

        <property name="todayCommission" type="java.math.BigDecimal" scale="2">
            <column name="today_commission" default="0"/>
        </property>

        <property name="lastDayCommission" type="java.math.BigDecimal" scale="2">
            <column name="last_day_commission" default="0"/>
        </property>

        <property name="totalConsumption" type="java.math.BigDecimal" scale="2">
            <column name="total_consumption" default="0"/>
        </property>

        <property name="todayConsumption" type="java.math.BigDecimal" scale="2">
            <column name="today_consumption" default="0"/>
        </property>

        <property name="lastDayConsumption" type="java.math.BigDecimal" scale="2">
            <column name="last_day_consumption" default="0"/>
        </property>

        <property name="bindDate" column="bind_date" type="java.util.Date"/>

        <property name="groupName" column="group_name" type="java.lang.String"/>

        <property name="rebateRatio" type="java.math.BigDecimal" scale="2">
            <column name="rebate_ratio" default="0"/>
        </property>

        <property name="daquCommission" type="java.math.BigDecimal" scale="2">
            <column name="daqu_commission" default="0"/>
        </property>

        <property name="daquTotalCommission" type="java.math.BigDecimal" scale="2">
            <column name="daqu_total_commission" default="0"/>
        </property>


    </class>
</hibernate-mapping>